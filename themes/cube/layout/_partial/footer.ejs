<!-- 网站信息 -->
<div>
  <span><i class="fa fa-copyright"></i> 2021-2022</span>
  <span class="space-mark">|</span>
  <% if (config.author) { %>
    <span><i class="fa fa-user-o"></i> <%= config.author %></span>
  <% } %>
  <span class="space-mark">|</span>
  <span><i class="fa fa-bar-chart"></i> 全站字数:</span>
  <span class="post-count"><%= totalcount(site) %></span>
</div>

<!-- 网站运行时长统计 -->
